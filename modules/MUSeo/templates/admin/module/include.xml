{* purpose of this template: modules xml inclusion template in admin area *}
<module id="{$item.id}" createdon="{$item.createdDate|dateformat}" updatedon="{$item.updatedDate|dateformat}">
    <id>{$item.id}</id>
    <name><![CDATA[{$item.name}]]></name>
    <controllerForView><![CDATA[{$item.controllerForView}]]></controllerForView>
    <controllerForSingleObject><![CDATA[{$item.controllerForSingleObject}]]></controllerForSingleObject>
    <parameterForObjects><![CDATA[{$item.parameterForObjects}]]></parameterForObjects>
    <nameOfIdentifier><![CDATA[{$item.nameOfIdentifier}]]></nameOfIdentifier>
    <extraIdentifier><![CDATA[{$item.extraIdentifier}]]></extraIdentifier>
</module>
